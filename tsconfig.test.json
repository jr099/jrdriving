{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": ".test-dist",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2020",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "src/lib/auth.ts",
    "src/lib/navigation.ts",
    "src/lib/supabase.ts",
    "src/lib/missions.ts",
    "src/lib/auth.test.ts",
    "src/routes.constants.ts",
    "src/routes.test.ts",
    "src/lib/queryClient.ts",
    "src/api/audit.ts",
    "src/api/quotes.ts",
    "src/pages/quoteForm.schema.ts",
    "src/pages/quoteForm.schema.test.ts",
    "src/vendor/**/*",
    "src/types/stubs.d.ts"
  ]
}
